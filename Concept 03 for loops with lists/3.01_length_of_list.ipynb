   "source": [
    "def length(my_list):\n",
    "    count=0\n",
    "# value of count function increases by one as we go through the elements of list\n",
    "    for i in my_list:\n",
    "        count+=1\n",
    "    return count\n",
    "\n",
    "my_list=[1,2,3,4,5,6,7,8,9,0]\n",
    "print(length(my_list))\n"
   ]
  }
